{
  "version": 2,
  "name": "sentinel-cli-website",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
  "outputDirectory": "build",
    { "src": "\/(.*)", "dest": "build/index.html" }
  ]
}
