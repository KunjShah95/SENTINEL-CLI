{
  "name": "sentinel-cli",
  "version": "1.9.0",
  "description": "ðŸ›¡ï¸ AI-Powered Code Guardian with GitHub App integration, automated PR reviews, security badges, intelligent caching, and comprehensive error handling. Supports 15+ analyzers, 3+ LLM providers (OpenAI, Gemini, Groq), and real-time webhook automation.",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "sentinel": "./src/core/cli.js"
  },
  "files": [
    "src/**/*",
    "dist/**/*",
    "frontend/build/**/*",
    "__tests__/**/*",
    "github-app.yml",
    "README.md",
    "IMPROVEMENTS.md",
    "QUICKSTART.md",
    ".env.example",
    ".sentinel.json.example"
  ],
  "scripts": {
    "start": "node src/core/cli.js",
    "launch": "node src/core/cli.js dashboard",
    "dev": "node --watch src/core/cli.js",
    "auth": "node src/core/cli.js auth",
    "config": "node src/core/cli.js config",
    "review": "node src/core/cli.js analyze",
    "security": "node src/core/cli.js security-audit",
    "full-scan": "node src/core/cli.js full-scan",
    "frontend": "node src/core/cli.js frontend",
    "backend": "node src/core/cli.js backend",
    "pre-commit": "node src/core/cli.js pre-commit --block",
    "install-hooks": "node src/core/cli.js install-hooks",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "build": "node src/core/build.js",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "dashboard": "node src/core/cli.js dashboard",
    "badge-server": "node src/core/cli.js badge-server",
    "webhook-server": "node src/core/cli.js webhook-server",
    "status": "node src/core/cli.js status",
    "benchmark": "node src/core/cli.js benchmark",
    "bench": "node src/core/cli.js benchmark --quick",
    "deploy": "npm run build:frontend && npx vercel --prod",
    "demo": "node src/core/demo.js",
    "prepublishOnly": "npm run lint && npm run build:all",
    "postversion": "git push && git push --tags",
    "postinstall": "echo \"\nâœ¨ Sentinel CLI installed! Run 'sentinel auth' to configure your API keys or 'sentinel dashboard' to see the UI.\n\""
  },
  "keywords": [
    "code-review",
    "automation",
    "security",
    "quality",
    "git",
    "cli",
    "ai",
    "static-analysis",
    "linter",
    "vulnerability",
    "dependency-check",
    "accessibility",
    "a11y",
    "wcag",
    "openai",
    "gemini",
    "groq",
    "llm",
    "pre-commit",
    "devops",
    "cicd",
    "caching",
    "badges",
    "github-app",
    "error-handling",
    "security-score",
    "webhook",
    "parallel-processing",
    "performance",
    "config-validation",
    "automated-pr-review",
    "docker-security",
    "secrets-detection",
    "typescript",
    "react",
    "vue"
  ],
  "author": "Kunj Shah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KunjShah95/SENTINEL-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/KunjShah95/SENTINEL-CLI/issues"
  },
  "homepage": "https://github.com/KunjShah95/SENTINEL-CLI#readme",
  "dependencies": {
    "@google/generative-ai": "^0.1.3",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.6.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "diff": "^5.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^1.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "figlet": "^1.7.0",
    "glob": "^10.3.10",
    "gradient-string": "^2.0.2",
    "groq-sdk": "^0.3.3",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "inquirer": "^9.2.12",
    "jshint": "^2.13.6",
    "marked": "^11.1.0",
    "ora": "^7.0.1",
    "semver": "^7.5.4",
    "simple-git": "^3.24.0",
    "sirv": "^3.0.2",
    "sql.js": "^1.10.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint-config-prettier": "^9.1.0",
